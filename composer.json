{
  "name": "mathias/parser-combinator",
  "version": "0.2",
  "description": "The easiest way to build robust parsers.",
  "authors": [
    {
      "name": "Mathias Verraes",
      "email": "mathias@verraes.net"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "vimeo/psalm": "^3.11",
    "mathiasverraes/uptodocs": "dev-main",
    "cypresslab/php-curry": "^0.5.0"
  },
  "autoload": {
    "psr-4": {
      "Mathias\\ParserCombinator\\": "src/"
    },
    "files": [
      "src/ParseResult/functions.php",
      "src/Predicates/predicates.php",
      "src/characters.php",
      "src/combinators.php",
      "src/numeric.php",
      "src/primitives.php",
      "src/recursion.php",
      "src/space.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Mathias\\ParserCombinator\\": "tests/"
    }
  }
}
